<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version2" level="2" version="2"
      xmlns:math="http://www.w3.org/1998/Math/MathML">
    <model sboTerm="SBO:0000004">
        <listOfCompartmentTypes>
            <compartmentType id="mitochondria"/>
        </listOfCompartmentTypes>
        <listOfSpeciesTypes>
            <speciesType id="Glucose"/> 
        </listOfSpeciesTypes>        
        <listOfCompartments>
            <compartment id="cell" size="0.013" compartmentType="mitochondria" outside="m"/>
            <compartment id="m" size="0.013" compartmentType="mitochondria"/>
        </listOfCompartments>
        <listOfSpecies>
            <species id="X0" compartment="cell" speciesType="Glucose"/>
            <species id="X1" compartment="cell" initialConcentration="0.013"/>
        </listOfSpecies>
        <listOfParameters>
          <parameter id="y" units="dimensionless" value="2"/>
        </listOfParameters>
        <listOfInitialAssignments>
            <initialAssignment symbol="X0" sboTerm="SBO:0000064">
                <math xmlns="http://www.w3.org/1998/Math/MathML">
                    <apply>
                        <times/>
                        <ci> y </ci>
                        <ci> X1 </ci>
                    </apply>
                </math>
            </initialAssignment>
         </listOfInitialAssignments>        
        <listOfConstraints>
            <constraint>
                <math xmlns="http://www.w3.org/1998/Math/MathML">
                    <apply>
                        <lt/>
                        <cn> 1 </cn>
                        <ci> cell </ci>
                    </apply>
                </math>
                <message>
                    <p xmlns="http://www.w3.org/1999/xhtml">
                    Species S1 is out of range 
                    </p>
                </message>
            </constraint>
        </listOfConstraints>        
        <listOfReactions>
            <reaction id="in" sboTerm="SBO:0000231">
                <listOfReactants>
                    <speciesReference species="X0" id="me" name="sarah"/>
                </listOfReactants>
                <kineticLaw>
                 <math xmlns="http://www.w3.org/1998/Math/MathML">
                    <apply>
                      <divide/>
                        <apply>
                          <times/>
                          <ci> v </ci>
                          <ci> X0 </ci>
                        </apply>
                        <ci> t </ci>  
                    </apply>
                </math>
                <listOfParameters>
                  <parameter id="v" units="litre"/>
                  <parameter id="t" units="second"/>
                </listOfParameters>
               </kineticLaw>
            </reaction>
        </listOfReactions>
    </model>
</sbml>
